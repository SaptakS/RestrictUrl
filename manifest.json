{
	"name" : "URL Restrictor",
	"version" : "1.0.0",
	"manifest_version":2,
	"description" : "Automatically block URL",
	"browser_action" : {
		"name" : "URL Restrictor",
		"default_icon": "icons/image19.png"
	},
	"background": {
		"page": "background.html"
	},
	"permissions" : ["tabs", "http://*/*", "https://www.google.com/*"],
	"icons" : {
		"16": "icons/image16.png",
		"48": "icons/image48.png",
		"128": "icons/image128.png"
	}
}